{
  "name": "night-patrol",
  "version": "2.0.0",
  "description": "Interactive CLI for Nightwatch Test Runner",
  "author": "Jahed Ahmed",
  "license": "MIT",
  "repository": "https://github.com/jahed/night-patrol",
  "keywords": [
    "nightwatch",
    "test",
    "cli",
    "interactive"
  ],
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "night-patrol": "bin/cli.js"
  },
  "scripts": {
    "start": "babel-node ./source/cli.js",
    "test:lint": "eslint ./source/",
    "test:unit": "jest",
    "test": "yarn test:lint && yarn test:unit",
    "prebuild": "rimraf dist && mkdir dist",
    "build": "BABEL_ENV=build babel --out-dir dist source",
    "watch": "yarn build --watch",
    "ready": "yarn test && yarn build",
    "push": "git push --follow-tags",
    "deploy": "yarn ready && standard-version && yarn publish && yarn push",
    "deps": "yarn upgrade-interactive --latest && yarn upgrade && git add yarn.lock '*package.json' && git commit -m 'build(deps): upgrade dependencies'"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "peerDependencies": {
    "nightwatch": "^1.0.0"
  },
  "dependencies": {
    "lodash": "4.17.11",
    "minimist": "1.2.0",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "require-dir": "1.2.0",
    "shelljs": "0.8.3",
    "stripcolorcodes": "0.1.0",
    "vorpal": "1.12.0"
  },
  "devDependencies": {
    "@commitlint/cli": "7.5.2",
    "@commitlint/config-conventional": "7.5.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-jest": "23.6.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-3": "6.24.1",
    "babel-register": "6.26.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb-base": "11.3.2",
    "eslint-plugin-import": "2.16.0",
    "husky": "1.3.1",
    "jest": "23.6.0",
    "nightwatch": "1.0.19",
    "rimraf": "2.6.3",
    "standard-version": "4.4.0"
  }
}
